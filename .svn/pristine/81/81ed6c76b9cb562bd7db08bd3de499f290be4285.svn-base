/* CSS Document */
* { margin:0; padding:0; border:0px none #FFFFFF;}
img{border:0;}
li{list-style:none;}
a { text-decoration:none; cursor: pointer;cursor: hand;}
body a{outline:none;blr:expression(this.onFocus=this.blur());}
.main{ width:100%; height:100%;}
.head{ width:100%; height:34px; line-height:34px; background-color:#333232; font-family:"宋体"; font-size:12px;}
.head_c{ width:990px; height:34px; margin:0 auto;}
.head_l{ width:470px; height:34px; float:left;}
.head_l ul li{ float:left; padding-right:8px; color:#bebebe;}
.head_l ul li a{ color:#FFF;}
.head_l ul li a:hover{ color:#F00;}
.head_l ul li span{ color:#FFF;}
.head_l ul li p{ font-size:14px; color:#00eaff; font-weight:bold;}
.head_l ul li dl dt{ float:left;}
.head_l ul li dl dd{ width:15px; height:18px; background-image:url(../images/validate/per_03.jpg); background-repeat:no-repeat; padding-left:23px;float:left; margin-top:8px; line-height:18px;}
.head_l ul li dl dd a{ color:#e5cc1e;}
.head_l ul li dl dd a:hover{ color:#e5cc1e; text-decoration:underline;}
.head_r{ width:310px; height:34px; float:right;}
.head_r ul{ margin-top:8px;}
.head_r ul a{ width:56px; height:18px; color:#FFF; display:block; font-size:12px; font-family:"宋体";}
.head_r ul li{ width:56px; height:18px; line-height:18px; float:left; margin-left:7px; margin-right:7px; text-align:center;}
.head_r ul li a:hover{ background-image:url(../images/validate/per_05.jpg); background-repeat:no-repeat; color:#FF0;}
.head_r ul li div{ background-image:url(../images/validate/per_08.gif); background-repeat:no-repeat; background-position:56px 8px; width:70px;_background-position:63px 7px;_width:75px;}
.headul{ display:none; width:70px; height:auto; background-color:#333232; position:absolute; padding-bottom:5px;}
.headul ul li{ color:#FFF; text-align:center; margin-bottom:5px; height:15px; line-height:15px;}
.headul ul li a{ color:#FFF;}
.headul ul li a:hover{background-image:url(../images/validate/per_05.jpg); background-repeat:no-repeat; color:#FF0;}
.yf_head_mess span a{ color:#3686e0;font-size:14px;}
.yf_head_mess span a:hover{ text-decoration:underline;}
.logo{ width:990px; margin:0 auto;}
.logo_top{ width:193px; height:37px; float:left; padding-top:20px; padding-right:20px;}
.logo_img{ width:193px; height:37px; float:left;}
.logo_down{ width:990px; height:45px; float:left;}
.logo_img ul li{float:left;}
.logo_l{ width:623px; height:45px; float:left; padding-top:12px;}
.logo_l ul{ padding-top:13px; margin-left:-10px;}
.logo_l_ul_a{ padding:6px 10px 6px 10px; display:block; color:#333; font-size:15px; font-family:"微软雅黑";}
.logo_l_ul_a:hover{ background-color:#d30000; color:#FFF; border-radius:5px;}
.logo_l_ul_aon{background-color:#d30000; color:#FFF; padding:6px 10px 6px 10px; display:block;font-size:15px; font-family:"微软雅黑"; border-radius:5px;}
.logo_l ul li{ float:left; margin-left:10px; margin-right:10px;}
.logo_r{ float:left; width:425px;height:56px; margin-top:35px; margin-left:50px;}
.logo_r ul li{ float:left; width:305px; color:#aba8a8; font-size:12px; font-family:"宋体"; padding-bottom:6px;}
.logo_rullia{ margin-left:4px; margin-right:4px;}
.logo_rullia:hover{ color:#333; text-decoration:underline;}
.logo_rulliaon{ color:#333; margin-left:4px; margin-right:4px;}
.sys_contentinput{  width:260px; height:30px; line-height:30px; font-size:14px; color:#666; border:1px solid #ccc; padding-left:10px; font-size:14px; margin-left:10px;}
.sys_content button{ width:40px; line-height:28px; font-size:14px; color:#fff; border:1px solid #ccc; background-color:#00b5e5; margin-left:10px;}
.sys_content button:hover{background-color:#C7E0F7;}
.logo_r ul li dl{ width:425px; }
.logo_r ul li dl dt{ float:left;}
.logo_r ul li dl dd{ float:left;}
.search{ width:340px; height:27px; border:1px solid #d3d3d3; color:#c4c2c2; font-size:12px; padding-left:8px; line-height:27px;}
.submit{ width:75px; height:29px; background-image:url(../images/validate/per_18.jpg); background-repeat:no-repeat;_margin-top:1px;cursor: pointer;cursor: hand;}
.submit:hover{ background-image:url(../images/validate/per_19.jpg)}
.ann{ width:100%; height:28px; border-top:1px solid #dcdcdc; background-color:#f3f3f3; font-size:14px; font-family:"微软雅黑"; color:#3686e0;}
.ann_c{ width:990px; height:28px; margin:0 auto; line-height:28px;}
.ann_c dl{ padding-left:10px;}
.ann_c dl dt{ color:#000; float:left; margin-right:5px;}
.ann_c dl dd{ float:left;}
.subject{ width:990px; height:auto; margin:20px auto;font-family:"微软雅黑"; padding-bottom:20px;}


/*我的客户*/
.subject_top{ width:988px; height:203px; border:1px solid #f1f1f1; background-color:#fafafa; float:left; margin-bottom:10px;}
.usercont{ float:left; width:763px; height:203px; margin-left:25px;_margin-left:10px;}
