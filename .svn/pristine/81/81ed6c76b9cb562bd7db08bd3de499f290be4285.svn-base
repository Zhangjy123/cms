.usercont4{ width:153px; height:37px; float:right; margin-right:130px; margin-top:20px;}
.subject_down{width:988px; height:auto;background-color:#fafafa; padding-top:20px; float:left; border-bottom:1px solid #f1f1f1;border-left:1px solid #f1f1f1;border-right:1px solid #f1f1f1;border-top:2px solid #1890cf; margin-bottom:20px;padding-bottom:20px;}
.addcustomer{ width:960px; height:25px; float:left; margin-left:20px; padding-bottom:20px;}
.addcustomer a{ margin-top:10px; display:block; float:left;}
.customer_sousuo{ float:right; padding-right:70px;}
.customer_sousuo ul li{ float:left;}
.coop_rxxf_text{ width:281px; height:36px; line-height:36px;border-bottom:1px solid #ccc;border-top:1px solid #ccc;border-left:1px solid #ccc; color:#b0afaf; font-size:14px; text-indent:15px;_text-indent:0px;}
.coop_rxxf_sub{ width:91px; height:38px; background-color:#3d86f4; background-image:url(../images/yfds_14.jpg); background-repeat:no-repeat; background-position:32px 7px;cursor: pointer;cursor: hand;*margin-top:1px;}
.customer{ width:950px; margin-left:15px; margin-top:15px; height:auto; float:left; margin-bottom:15px;}
.customer_top{ width:950px; height:20px; float:left; font-size:15px;}
.customer_top dt{ float:left; margin-right:15px; color:#F00; width:100px;_margin-right:5px;}
.customer_top dd{ float:left; margin-right:15px;_margin-right:5px;}
.customer_name{ width:90px;}
.customer_type{ width:70px;}
.customer_syname{width:100px;}
.customer_contact{ width:60px;}
.customer_top dd span{ color:#1155cc; font-weight:bold;}
.customer_top dd a{ margin-right:7px;}
.customer_cont{ width:925px; height:auto;float:left; margin-top:15px; font-size:12px; margin-left:15px;}
.customer_cont dt{ width:925px; color:#424242; line-height:180%;}
.customer_cont dd{ width:925px; color:#7a7979; margin-bottom:10px;}
.customer_cont dd div{ float:right;}

 

/*发布合作信息*/
.coopform{ width:890px; height:660px; margin:20px auto;}
.coopform ul li{ padding-bottom:25px; font-size:15px; width:890px; float:left;}
.coopformspan{ width:85px; height:auto; color:#424242;float:left; text-align:right; padding-right:5px;white-space: nowrap;}
.coopformx{color:#f00;text-align:right;}
.coopform ul li dl{ float:left; margin-top:-2px; margin-right:-30px;}
.coopform ul li dl dt{ float:left;}
.coopform ul li div{ float:left;}
.coopform ul li div span{ float:left;white-space: nowrap;}
.coopform ul li div div{ width:39px; height:20px;}
.coopform ul li div input{ margin-right:20px;}
.coopform ul li textarea{ width:558px; height:90px; background-image:url(../images/validate/precoop_03.jpg); background-repeat:no-repeat; padding-left:12px;resize:none;float:left;}
.coopform ul li a{ color:#00F;}
.coopform ul li a:hover{ text-decoration:underline;}
.coopformdiv{ width:118px; height:28px; line-height:28px; border:1px solid #ccc;float:left;}
.coopformmin{ width:74px; height:28px; line-height:28px; border:1px solid #ccc;float:left;}
.coopformtext{ border:1px solid #ccc; width:345px; height:28px; line-height:28px;float:left;}
.coopformselect{ width:164px; height:28px; line-height:28px; border:1px solid #ccc; margin-right:20px; text-align:center;float:left;_margin-top:2px;}
.coopformselect option{height:28px; line-height:28px; padding-right:16px;}
.coopformradio{ margin-left:5px; margin-right:5px;}
.coopformbox{margin-left:5px; margin-right:5px;}
.coopformyz{width:118px; height:38px; line-height:38px; border:1px solid #ccc; float:left; margin-top:-8px;}
.coopformimg{ width:100px; height:38px; line-height:38px; border:1px solid #ccc; margin-left:10px; margin-right:10px; margin-top:-8px;}
.coopformsub{ width:350px; height:50px; padding-left:90px;}
.coopformsub input{ width:350px; height:50px; border-radius:3px; font-size:18px; text-align:center; line-height:50px; color:#FFF; background-color:#3f89ec;}
.coopformsub input:hover{ background-color:#6a92db;}



/*基本资料*/
.userdata{ padding-left:130px;}
.userdata ul li{ padding-top:5px; padding-bottom:5px; font-size:14px; float:left; width:750px;}
.userdata_title{ width:95px;height:36px; line-height:36px; float:left; text-align:right;}
.userdata_cont{ width:310px;height:36px; line-height:36px; float:left;}
.userdata_cont select{background-color:#f3f9fc; border:1px solid #ccc; border-radius:3px; float:left; margin-right:10px; width:100px;text-align:left; margin-top:10px;}
.userdata_prompt{width:auto;height:36px; line-height:36px; float:left; color:#666; font-size:12px;}
.userdataname{ width:198px; height:36px; line-height:36px; background-color:#f3f9fc; border:1px solid #ccc; border-radius:3px; float:left; margin-right:10px;}
.userdatatext{ width:298px; height:20px; line-height:20px; background-color:#fff; border:1px solid #ccc; margin-right:10px;}
.userdata_box{width:400px;height:36px; line-height:36px; float:left;}
.userdatasub{width:296px; height:43px; margin-left:180px; margin-top:50px; float:left;_margin-left:80px;}
