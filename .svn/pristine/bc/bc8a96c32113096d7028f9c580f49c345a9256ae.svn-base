/* CSS Document */
* { margin:0; padding:0; border:0px none #FFFFFF;font-family:"宋体";}
img{border:0;}
li{list-style:none;}
a { text-decoration:none; cursor: pointer;cursor: hand;}
button{ cursor: pointer;cursor: hand;}
body a{outline:none;blr:expression(this.onFocus=this.blur());}
.cle:after {content:"."; display:block; height:0; clear:both; visibility:hidden;}
.top{ width:100%; height:79px; background-image:url(../images/khimg_r1_c10.jpg); background-repeat:repeat-x; }
.cont{ width:100%; height:auto;}
.logo{ float:left; font-size:25px; color:#FFF;line-height:45px; padding-top:10px;padding-left:40px;}
.logo ul li{ float:left; padding-right:5px;}
.out{ float:right; width:245px; height:20px; line-height:20px; color:#FFF;padding-top:20px;}
.out a{ color:#FFF; font-size:14px; font-weight:600;}
.out a:hover{ text-decoration:underline;}
.out ul li{ float:left; padding-right:5px}
.out div{ font-size:14px; color:#8bef16;}
.menu{ float:left; width:100%; margin-top:-16px;}
.menu ul{padding-left:120px;}
.menu ul li{ float:left; padding-right:10px;}
.menu ul li a{ font-size:15px; font-weight:600;}
.menu ul li ul{ position:absolute; width:126px;padding-left:0px; background-color:#92C5EB;}
.menu ul li ul li{ width:126px;padding-right:0px; padding-top:3px; padding-bottom:3px;}
.menu_a{ width:126px; height:22px; line-height:22px; color:#FFF; text-align:center; background-image:url(../images/khimg_r4_c5.jpg); background-repeat:no-repeat; display:block; margin-top:11px;}
.menu_a:hover{ width:126px; height:30px; line-height:30px; background-image:url(../images/khimg_r3_c3.jpg); background-repeat:no-repeat; text-align:center;margin-top:3px;}
.menu_aon{ width:126px; height:30px; line-height:30px; color:#000; background-image:url(../images/khimg_r3_c7.jpg); background-repeat:no-repeat;display:block; text-align:center; margin-top:3px;}
.menu_ae{width:126px; height:22px; line-height:22px; font-size:16px; text-align:center; color:#FFF; display:block;}
.menu_ae:hover{ background-color:#09C;}.cont h4{ color:#f00; font-size:14px; padding-left:80px;}
.adduser{ padding-top:20px; padding-left:80px;}
.adduser a{ display:block; background-color:#09F; width:120px; height:30px; line-height:30px; color:#FFF; font-size:14px; text-align:center; border-radius:3px;}
.adduser a:hover{ background-color:#9CF;}
.select{ width:90%;; height:auto; margin: 20px auto; background-color:#f3f3f3; padding:10px;}
.name_ss{ float:left; padding-right:10px; height:20px; padding-bottom:10px;}
.name_ss input{ border:1px solid #ccc; width:180px; height:20px; line-height:20px; padding-left:10px;}
.name_ss select{ border:1px solid #ccc;}
.list{ width:90%;; height:auto; margin: 20px auto; background-color:#f9f9f9;padding:10px;}
.list table{  margin-bottom:20px;}
.list table td{ text-align:center;padding-bottom:5px;}
.list .table_a{ color:#F00; margin-right:5px; width:20px;}
.list .table_a:hover{ text-decoration:underline;}
.list ul{ float:right; padding-right:20px; padding-bottom:10px; width:500px;}
.list_ul_a{ padding:2px; background-color:#FFF; border:1px solid #09F; color:#06C; border-radius:3px;}
.list_ul_a:hover{ background-color:#06C; color:#FFF;}
.list_ul_aon{padding:2px; background-color:#FFF; border:1px solid #ccc; color:#06C;background-color:#06C; color:#fff;border-radius:3px;}
.list ul li{ float:right;padding-left:5px;}
.followdiv_input{ padding-left:30%; padding-top:10px; padding-bottom:10px; margin:auto;}
.followdiv_input input{ width:40%; height:20px; line-height:20px; text-align:left; padding-left:10px; border:1px solid #ccc;float:left;}
.followdiv_input a{ color:#FFF; background-color:#090; display:block; width:70px; height:22px; line-height:22px; text-align:center; font-size:12px; float:left;}
.followdiv_cpnt{ padding-bottom:5px; padding-top:5px;width:90%; margin:auto; font-size:14px; text-align:left;}
.followdiv_cpnt span{ color:#F00; padding-right:10px;}
.cun{ width:40px; line-height:22px; height:22px; font-size:12px; color:#FFF; background-color:#06C; display:block; text-align:center; margin-top:1px;}
.cun:hover{ color:#FFF; background-color:#09C;}
.cun:active{ background-color:#666; color:#CCC;}

/*弹出层*/
.imgBox{width:600px; height:auto;border:1px solid #ABD0F3; border-radius:3px;}
.imgBox_b{ width:560px; height:27px; float:left;background-color:#C7E0F7; font-size:16px; padding-top:5px;padding-left:20px; padding-right:20px;}
.imgBox_b_l{ float:left;}
.imgBox_b_r{ float:right; padding-top:6px;*margin-top:0px;}
#colsebutton{ width:10px; height:8px; background-image:url(../images/coopdisdiv_03.jpg); background-repeat:no-repeat;cursor: pointer;cursor: hand; display:block;}
.imgBox_cont{ float:left; height:auto; padding-top:20px; padding-bottom:20px; background-color:#FFF; width:560px;padding-left:20px; padding-right:20px;}

