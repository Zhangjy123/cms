package com.yofang.cms.enums;

public enum RoleType {
	/**
	 * 销售
	 */
	SALES("销售",1),
	CHANNEL_SALES("渠道销售",2),
	MANAGER("经理",3),
	PRODUCT_DIRECTOR("项目总监",4),
	FINANCE("财务",6),
	TOP_MANAGER("总经理",6),
	ADMIN("系统管理员",6);
	//角色类型
	private String name;
	//序号
	private int value;
	
	private RoleType(String name, int value) {
		this.name = name;
		this.value = value;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}
}
